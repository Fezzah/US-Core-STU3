			<max value="1"/>
			<type>
				<code value="CodeableConcept"/>
			</type>
			<constraint>
				<extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bestpractice">
					<valueBoolean value="true"/>
				</extension>
				<key value="us-core-1"/>
				<severity value="warning"/>
				<human value="SHOULD have a translation to the NDC value set"/>
				<expression value="coding.where(system='http://hl7.org/fhir/sid/ndc').empty()"/>
