						<reference value="http://hl7.org/fhir/us/core/ValueSet/simple-language"/>
					</valueReference>
				</extension>
				<strength value="extensible"/>
				<valueSetReference>
					<reference value="http://hl7.org/fhir/us/core/ValueSet/simple-language"/>
				</valueSetReference>
			</binding>
			<mapping>
				<identity value="argonaut-dq-dstu2"/>
				<map value="Patient.communication.language"/>
