			</binding>
			<mapping>
				<identity value="argonaut-dq-dstu2"/>
				<map value="Procedure.code"/>
			</mapping>
		</element>
		<element id="Procedure.subject">
			<path value="Procedure.subject"/>
			<min value="1"/>
			<max value="1"/>
